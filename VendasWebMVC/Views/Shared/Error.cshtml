@model SalesWebMVC.Models.ViewModels.ErrorViewModel
@{
    ViewData["Title"] = "Erro";
}

<h1 class="text-danger">Ops :(</h1>
<h2 class="text-danger">@Model.Message</h2>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Identificação de requisição:</strong> <code>@Model.RequestId</code>
    </p>
}


